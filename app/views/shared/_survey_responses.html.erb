<h1>My Responses</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Response</th>
    </tr>
  </thead>
  <tbody>
    <% @survey_responses.each do |survey_response| %>
      <tr>
        <td><%= survey_response.date.strftime('%Y-%m-%d') %></td>
        <td><%= survey_response.response.to_json %></td>
      </tr>
    <% end %>
  </tbody>
</table>